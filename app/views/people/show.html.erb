<div class="panel panel-default">
	<div class="panel panel-heading">
		People
	</div>
	<div class="panel panel-body">
		<p>
  		<strong>Name:</strong>
  		<%= @person.name %>
		</p>

		<p>
		  <strong>Phone:</strong>
		  <%= @person.phone %>
		</p>

		<p>
		  <strong>Designation:</strong>
		  <%= @person.designation %>
		</p>

		<p>
		  <strong>Organization:</strong>
		  <%= @person.organization %>
		</p>
	</div>
	<div class="panel panel-footer">
		<%= link_to 'People', people_url, class: 'btn btn-default' %>
	</div>
</div>